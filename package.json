  },
  "devDependencies": {
    "istanbul": "^0.2.16",
    "jshint": "^2.4.4",
    "jshint-full-path": "^1.1.1",
    "lru-cache": "*",
    "mocha": "^1.21.5",
    "should": "^2.1.1"
  }
}
